{
    "manifest_version": 3,
    "name" : "Local Time Everywhere",
    "version" : "0.1.0",
    "description" : "Converts all time to your local time",
    "permissions" : [
        "tabs",
        "activeTab"
    ],
    "background" :{
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Local Time Everywhere",
        "default_popup": "index.html"
    }
}
